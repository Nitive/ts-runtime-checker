{
  "compileOnSave": false,
  "compilerOptions": {
    "lib": [
      "es2015.collection",
      "es2015.promise",
      "es2015",
      "es2016",
      "es5"
    ],
    "types": [
      "node"
    ],
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "outDir": "./dist/",
    "rootDir": "./src/",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5"
  },
  "typeroots": [
    "./node_modules/@types"
  ],
  "filesGlob": [
    "./**/*.ts*"
  ]
}
